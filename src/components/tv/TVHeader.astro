<div class="top-header-bar" id="top-header-bar">
    <div class="top-header-bar__content">
        <div class="top-header-bar__status">
            <span class="status-light status-light--ok" id="status-mainframe" title="MONITOR"></span>
            <span class="status-light status-light--ok" id="status-network" title="WIPES"></span>
            <span class="status-light status-light--ok" id="status-database" title="BOTTLES"></span>
            <span class="status-light status-light--warning" id="status-shield" title="NAPS"></span>
        </div>
        <div class="top-header-bar__countdown">
            <span class="top-header-bar__label">EXPECTED ARRIVAL</span>
            <span class="top-header-bar__time" id="midnight-time"
                >T-MINUS 30 DAYS</span
            >
        </div>
        <div class="top-header-bar__teams-count">
            <span class="top-header-bar__teams-label">TEAMS</span>
            <span class="top-header-bar__teams-value" id="status-team-count">0</span>
        </div>
    </div>
</div>
